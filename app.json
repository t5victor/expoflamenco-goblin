{
  "expo": {
    "name": "Expo Stats",
    "slug": "expo-goblin",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/LogoAppIOS.png",
    "scheme": "expogoblin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expoflamenco.goblin",
      "companyName": "Expoflamenco",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "template": "./web/index.html"
    },
    "plugins": [
      [
      "expo-router",
      {
        "origin": "https://expoflamenco.com"
      }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "040d843d-7959-48ae-8ec1-977b5756d9dc"
      }
    },
    "owner": "expovictor6",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/040d843d-7959-48ae-8ec1-977b5756d9dc"
    }
  }
}
